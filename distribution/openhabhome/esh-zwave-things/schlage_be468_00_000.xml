<?xml version="1.0" encoding="UTF-8"?><thing:thing-descriptions bindingId="zwave"xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"xmlns:thing="http://eclipse.org/smarthome/schemas/zwave-thing-description/v1.0.0"xsi:schemaLocation="http://eclipse.org/smarthome/schemas/zwave-thing-description/v1.0.0 http://eclipse.org/smarthome/schemas/zwave-thing-description/v1.0.0"><thing-type id="schlage_be468_00_000"><label>Schlage BE468</label><description><![CDATA[Touchscreen Deadbolt<br><h1>Device Overview</h1><p>Brief description here</p><h1>Inclusion and Exclusion</h1><p>To include the device into the network...</p><p>To exclude the device from the network...</p>]]></description><channels><!-- The following is a random selection of channels - it must be changed! --><channel id="brightness-1" typeId="brightness"><label>Dimmer 1</label><properties><property name="binding:*:HSBType">COLOR;colorHue=true</property><property name="binding:*:PercentType">SWITCH_MULTILEVEL,BASIC</property><property name="binding:Command:OnOffType">SWITCH_MULTILEVEL</property><property name="binding:*:OnOffType">SWITCH_BINARY:1,BASIC:1</property><property name="name=binding:*:OnOffType">SWITCH_BINARY:2,BASIC:2</property></properties></channel><channel id="switch-1" typeId="switch"><label>Switch 1</label><properties><property name="binding:*:HSBType">COLOR;colorHue=true</property><property name="binding:*:PercentType">SWITCH_MULTILEVEL,BASIC</property><property name="binding:Command:OnOffType">SWITCH_MULTILEVEL</property><property name="binding:*:OnOffType">SWITCH_BINARY:1,BASIC:1</property><property name="name=binding:*:OnOffType">SWITCH_BINARY:2,BASIC:2</property></properties></channel><!-- Remove if this is not a battery device --><channel id="battery-level" typeId="system.battery-level"><properties><property name="commandClass">BATTERY</property></properties></channel></channels><properties><property name="vendor">Schlage</property><property name="model">BE468</property><property name="manufacturerId">003B</property><property name="manufacturerRef">6349:5044</property><property name="commandClass:ASSOCIATION:default">1</property></properties><config-description><parameter-group name="configuration"><context>setup</context><label>Configuration Parameters</label></parameter-group><parameter-group name="association"><context>link</context><label>Association Groups</label></parameter-group><!-- Remove if this is not a battery device --><parameter-group name="wakeup"><context>sleep</context><label>Wakeup Configuration</label></parameter-group><parameter name="config_3_1" type="integer" groupName="configuration"><label>3: Beeper</label><description><![CDATA[Audio feedback when keypad pressed during normal operation]]></description><default>255</default><options><option value="0">Disable</option><option value="255">Enable</option></options></parameter><parameter name="config_4_1" type="integer" groupName="configuration"><label>4: Vacation Mode</label><description><![CDATA[Prevents all user codes from unlocking the deadbolt]]></description><default>0</default><options><option value="0">Disable</option><option value="255">Enable</option></options></parameter><parameter name="config_5_1" type="integer" groupName="configuration"><label>5: Lock & Leave</label><description><![CDATA[Press the Schlage button to lock the deadbolt]]></description><default>255</default><options><option value="0">Disable</option><option value="255">Enable</option></options></parameter><parameter name="config_15_1" type="integer" groupName="configuration"><label>15: Auto Lock</label><description><![CDATA[Automatically locks 30 seconds after unlocking]]></description><default>0</default><options><option value="0">Disable</option><option value="255">Enable</option></options></parameter><parameter name="config_16_1" type="integer" min="4" max="8" groupName="configuration"><label>16: User Code PIN Length</label><description><![CDATA[User codes can be 4-8 digits. Changing length will delete all existing codes.]]></description><default>4</default></parameter><parameter name="group_1" type="integer" multiple="true" groupName="association"><label>Group 1</label><description><![CDATA[***Add description here***]]></description><multipleLimit>2</multipleLimit></parameter><!-- Remove the wakeup if this is not a battery device --><parameter name="wakeup_interval" type="integer" groupName="wakeup"><label>Wakeup Period</label><description>Set the wakeup period in seconds</description></parameter><parameter name="wakeup_node" type="integer" groupName="wakeup" readOnly="true"><label>Wakeup Node</label><description>The node to which wakeup messages will be sent</description></parameter></config-description></thing-type></thing:thing-descriptions>