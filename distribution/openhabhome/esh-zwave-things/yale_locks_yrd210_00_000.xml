<?xml version="1.0" encoding="UTF-8"?><thing:thing-descriptions bindingId="zwave"xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"xmlns:thing="http://eclipse.org/smarthome/schemas/zwave-thing-description/v1.0.0"xsi:schemaLocation="http://eclipse.org/smarthome/schemas/zwave-thing-description/v1.0.0 http://eclipse.org/smarthome/schemas/zwave-thing-description/v1.0.0"><thing-type id="yalelocks_yrd210_00_000"><label>Yale Locks YRD210</label><description><![CDATA[Push Button Deadbolt<br><h1>Device Overview</h1><p>Brief description here</p><h1>Inclusion and Exclusion</h1><p>To include the device into the network...</p><p>To exclude the device from the network...</p>]]></description><channels><!-- The following is a random selection of channels - it must be changed! --><channel id="brightness-1" typeId="brightness"><label>Dimmer 1</label><properties><property name="binding:*:HSBType">COLOR;colorHue=true</property><property name="binding:*:PercentType">SWITCH_MULTILEVEL,BASIC</property><property name="binding:Command:OnOffType">SWITCH_MULTILEVEL</property><property name="binding:*:OnOffType">SWITCH_BINARY:1,BASIC:1</property><property name="name=binding:*:OnOffType">SWITCH_BINARY:2,BASIC:2</property></properties></channel><channel id="switch-1" typeId="switch"><label>Switch 1</label><properties><property name="binding:*:HSBType">COLOR;colorHue=true</property><property name="binding:*:PercentType">SWITCH_MULTILEVEL,BASIC</property><property name="binding:Command:OnOffType">SWITCH_MULTILEVEL</property><property name="binding:*:OnOffType">SWITCH_BINARY:1,BASIC:1</property><property name="name=binding:*:OnOffType">SWITCH_BINARY:2,BASIC:2</property></properties></channel><!-- Remove if this is not a battery device --><channel id="battery-level" typeId="system.battery-level"><properties><property name="commandClass">BATTERY</property></properties></channel></channels><properties><property name="vendor">Yale Locks</property><property name="model">YRD210</property><property name="manufacturerId">0129</property><property name="manufacturerRef">0004:AA00</property><property name="commandClass:ASSOCIATION:default">1</property></properties><config-description><parameter-group name="configuration"><context>setup</context><label>Configuration Parameters</label></parameter-group><parameter-group name="association"><context>link</context><label>Association Groups</label></parameter-group><!-- Remove if this is not a battery device --><parameter-group name="wakeup"><context>sleep</context><label>Wakeup Configuration</label></parameter-group><parameter name="config_1_1" type="integer" groupName="configuration"><label>1: Audio Mode</label><description><![CDATA[]]></description><default>3</default><options><option value="1">Silent</option><option value="2">Low</option><option value="3">High</option></options></parameter><parameter name="config_2_1" type="integer" groupName="configuration"><label>2: Auto relock</label><description><![CDATA[]]></description><default>255</default><options><option value="0">Off</option><option value="255">On</option></options></parameter><parameter name="config_3_1" type="integer" min="5" max="255" groupName="configuration"><label>3: Relock time</label><description><![CDATA[Seconds; after successful code entry and unit unlocks, it will automatically re-lock after specified time]]></description><default>30</default></parameter><parameter name="config_4_1" type="integer" min="1" max="7" groupName="configuration"><label>4: Wrong Code Entry Limit</label><description><![CDATA[The number of invalid code entries lock will accept before sending TAMPER Alarm.
			When number of wrong code entries is exceeded, lock will disable keypad for the time specified by Shutdown Time parameter.]]></description><default>5</default></parameter><parameter name="config_5_1" type="integer" groupName="configuration"><label>5: Language</label><description><![CDATA[]]></description><default>1</default><options><option value="1">English</option><option value="2">Spanish</option><option value="3">French</option></options></parameter><parameter name="config_7_1" type="integer" min="1" max="255" groupName="configuration"><label>7: Shutdown Time</label><description><![CDATA[Number of seconds unit will be inoperable after number of wrong code entries is exceeded.]]></description><default>30</default></parameter><parameter name="config_8_1" type="integer" groupName="configuration"><label>8: Operating Mode</label><description><![CDATA[Normal Mode</br>
Vacation Mode - all user codes disabled</br>
Privacy Mode - all user codes disabled and RF Lock/Unlock disabled]]></description><default>0</default><options><option value="0">Normal</option><option value="1">Vacation</option><option value="2">Privacy</option></options></parameter><parameter name="group_1" type="integer" multiple="true" groupName="association"><label>Alarm Reports</label><description><![CDATA[Alarm reports are sent out unsolicited to devices included in the association group.]]></description><multipleLimit>4</multipleLimit></parameter><!-- Remove the wakeup if this is not a battery device --><parameter name="wakeup_interval" type="integer" groupName="wakeup"><label>Wakeup Period</label><description>Set the wakeup period in seconds</description></parameter><parameter name="wakeup_node" type="integer" groupName="wakeup" readOnly="true"><label>Wakeup Node</label><description>The node to which wakeup messages will be sent</description></parameter></config-description></thing-type></thing:thing-descriptions>